<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dabizi.point.model.PointInfo" table="point_info">
		<id name="id" type="java.lang.Integer" column="point_info_id">
			<generator class="identity"></generator>
		</id>
		
		<property name="numId" type="java.lang.String">
			<column name="numId" length="100" />
		</property>
		
		<property name="longitude" type="java.lang.String">
			<column name="longitude" length="50" />
		</property>
		
		<property name="latitude" type="java.lang.String">
			<column name="latitude" length="50" />
		</property>
	</class>
</hibernate-mapping>
