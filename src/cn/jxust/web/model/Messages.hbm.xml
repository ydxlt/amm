<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="cn.jxust.web.model.Messages" table="messages">
		<id name="messageId" type="java.lang.String" column="message_id">
			<generator class="uuid.hex" />
		</id>
		<property name="title" type="java.lang.String">
			<column name="title" length="100" />
		</property>
		<property name="content" type="java.lang.String">
			<column name="content" length="1000" />
		</property>
		<property name="ltime" type="java.lang.String">
			<column name="ltime" length="20" />
		</property>
		<property name="messName" type="java.lang.String">
			<column name="mess_name" />
		</property>
		<property name="recontent" type="java.lang.String">
			<column name="recontent" length="1000" />
		</property>
		<property name="replyor" type="java.lang.String">
			<column name="replyor" length="19" />
		</property>
		<property name="replyFlag" type="java.lang.String">
			<column name="reply_flag" length="2" />
		</property>
	</class>
</hibernate-mapping>
